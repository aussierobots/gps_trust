# config/gpstrust.env.example
#
# Example environment for running gps_trust as a systemd service
# Copy this to /etc/gpstrust.env (or similar) on the station and edit.

# ROS 2 workspace
ROS2_WS="/home/gpstrust/gps_trust"
SETUP_BASH="/home/gpstrust/gps_trust/install/setup.bash"

# GPS Trust / API
GPS_TRUST_DEVICE_API_KEY="REPLACE_ME_WITH_REAL_API_KEY"

# NTRIP configuration
NTRIP_HOST="ntrip.data.gnss.ga.gov.au"
NTRIP_PORT="443"
NTRIP_USERNAME="REPLACE_ME"
NTRIP_PASSWORD="REPLACE_ME"
NTRIP_MOUNTPOINT="REPLACE_ME"
USE_HTTPS="true"

# Log and PID directories used by gpstrust.sh
LOG_DIR="/var/log/gpstrust"
PID_DIR="/run/gpstrust"
